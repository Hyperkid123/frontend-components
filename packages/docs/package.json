{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate:components:docs && next dev",
    "build": "npm run generate:components:docs && next build",
    "start": "npm run generate:components:docs && next start",
    "generate:components": "react-docgen ../components/src -x js --out component-docs.json --pretty /dev/null 2>&1",
    "generate:components:docs": "npm run generate:components && node docs-generator.js"
  },
  "dependencies": {
    "@patternfly/react-core": "^4.79.2",
    "@patternfly/react-icons": "^4.7.18",
    "@redhat-cloud-services/frontend-components": "^2.5.1",
    "next": "10.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.0.3",
    "@zeit/next-css": "^1.0.1",
    "next-transpile-modules": "^6.0.0",
    "raw-loader": "^4.0.2",
    "react-docgen": "^5.3.1",
    "react-jss": "^10.5.0",
    "url-loader": "^4.1.1"
  }
}
